on Syslisp;

internal WArray BPS[BPSSize];

exported WVar NextBPS = &BPS[0] + 8#1000000,
	      LastBPS = &BPS[BPSSize] + 8#1000000;

exported WConst Heap = 8#2000000;

off Syslisp;

END;
